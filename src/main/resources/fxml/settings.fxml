<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ca.ciccc.silverBullet.controller.SettingsController">
   <children>
      <StackPane layoutY="30.0">
         <children>
            <Text fill="#d01313" strokeType="OUTSIDE" strokeWidth="0.0" text="SETTINGS">
               <font>
                  <Font size="63.0" />
               </font>
            </Text>
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="80.0" opacity="0.4" stroke="WHITE" strokeType="INSIDE" width="900.0" />
         </children>
      </StackPane>
      <VBox alignment="CENTER" layoutX="131.0" layoutY="203.0">
         <effect>
            <DropShadow />
         </effect>
         <children>
            <HBox prefHeight="30.0">
               <children>
                  <Label prefWidth="400.0" text="Players" />
                  <ComboBox prefWidth="150.0" />
               </children>
            </HBox>
            <HBox prefHeight="30.0">
               <children>
                  <Label prefWidth="400.0" text="Sizse of Game Board" />
                  <ComboBox prefWidth="150.0" />
               </children>
            </HBox>
            <HBox prefHeight="30.0">
               <children>
                  <Label prefWidth="400.0" text="Stage Level" />
                  <ComboBox prefWidth="150.0" visibleRowCount="3" />
               </children>
            </HBox>
         </children>
      </VBox>
      <HBox layoutX="223.0" layoutY="491.0">
         <children>
            <StackPane onMouseClicked="#onStartClicked" onMouseEntered="#OnEntered" onMouseExited="#OnExited" onMousePressed="#OnPressed">
               <children>
                  <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="80.0" stroke="BLACK" strokeType="INSIDE" width="300.0" />
                  <Text fill="#5e5e5e" strokeType="OUTSIDE" strokeWidth="0.0" text="START">
                     <font>
                        <Font name="STHeiti" size="40.0" />
                     </font>
                  </Text>
               </children>
            </StackPane>
            <StackPane onMouseClicked="#onBackToMenuClicked" onMouseEntered="#OnEntered" onMouseExited="#OnExited" onMousePressed="#OnPressed">
               <children>
                  <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="80.0" stroke="BLACK" strokeType="INSIDE" width="300.0" />
                  <Text fill="#5e5e5e" strokeType="OUTSIDE" strokeWidth="0.0" text="BACK TO MENU">
                     <font>
                        <Font name="STHeiti" size="40.0" />
                     </font>
                  </Text>
               </children>
            </StackPane>
         </children>
      </HBox>
   </children>
</AnchorPane>
